{
  "name": "renderkid",
  "version": "0.2.0-beta.10",
  "description": "(work in progress)",
  "main": "lib/RenderKid.js",
  "dependencies": {
    "CSSselect": "~0.3.11",
    "htmlparser2": "~3.3.0",
    "joiful-experiments": "~1.2.2",
    "utila": "~0.3",
    "dom-converter": "~0.1"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-changes": "^1.3.4",
    "chai-fuzzy": "^1.4.0",
    "coffee-script": "^1.8.0",
    "jitter": "^1.3.0",
    "mocha": "^2.0.1",
    "mocha-pretty-spec-reporter": "0.1.0-beta.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0"
  },
  "scripts": {
    "test": "mocha \"test/**/*.coffee\"",
    "test:watch": "mocha \"test/**/*.coffee\" --watch",

    "compile": "coffee --bare --compile --output ./lib ./src",
    "compile:watch": "jitter src lib -b",

    "watch": "npm run compile:watch & npm run test:watch",
    "winwatch": "start/b npm run compile:watch & npm run test:watch",

    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AriaMinaei/RenderKid.git"
  },
  "bugs": {
    "url": "https://github.com/AriaMinaei/RenderKid/issues"
  },
